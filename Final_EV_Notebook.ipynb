    "# Initialize the XGBoost Regressor\n",
    "xg_reg = xgb.XGBRegressor(random_state=2024)\n",
    "\n",
    "# Define the parameter grid\n",
    "param_grid = {\n",
    "    'n_estimators': [50, 100, 200],\n",
